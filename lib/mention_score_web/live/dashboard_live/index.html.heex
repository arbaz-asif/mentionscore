<!-- lib/your_app_web/live/dashboard_live/index.html.heex -->
<div class="min-h-screen bg-white text-black">
  <div class="flex">
    <!-- Sidebar -->
    <div class="w-80 min-h-screen bg-gray-50 border-r border-gray-200">
      <div class="p-6">
        <!-- Logo -->
        <%!-- <div class="mb-8"> --%>
        <%!-- <h1 class="text-2xl font-bold text-black">
            MentionScore
          </h1> --%>
        <a href="/dashboard">
          <img src={~p"/images/ms-logo.png"} class="mx-auto h-12" alt="Logo" />
        </a>
         <%!-- </div> --%>
        
<!-- Navigation -->
        <nav class="space-y-2 mb-8">
          <button
            phx-click="change_tab"
            phx-value-tab="score"
            class={tab_class(@active_tab, "score")}
          >
            <.icon name="hero-magnifying-glass" class="w-5 h-5" />
            <span class="font-medium">Get Score</span>
          </button>

          <button
            phx-click="change_tab"
            phx-value-tab="chat"
            class={tab_class(@active_tab, "chat")}
          >
            <.icon name="hero-chat-bubble-left-right" class="w-5 h-5" />
            <span class="font-medium">Geora</span>
          </button>

          <button
            phx-click="change_tab"
            phx-value-tab="profile"
            class={tab_class(@active_tab, "profile")}
          >
            <.icon name="hero-information-circle" class="w-5 h-5" />
            <span class="font-medium">Profile</span>
          </button>

          <button
            phx-click="change_tab"
            phx-value-tab="credits"
            class={tab_class(@active_tab, "credits")}
          >
            <.icon name="hero-credit-card" class="w-5 h-5" />
            <span class="font-medium">Buy Credits</span>
          </button>

          <%!-- <button
  phx-click="change_tab"
  phx-value-tab="users"
  class={tab_class(@active_tab, "users")}
>
  <.icon name="hero-users" class="w-5 h-5" />
  <span class="font-medium">Users</span>
</button> --%>
        </nav>
        
<!-- Profile Section -->
        <div class="absolute bottom-6 left-6 w-[270px]">
          <div class="relative">
            <div
              phx-click="toggle_profile_dropdown"
              class="bg-white border border-gray-200 rounded-xl p-4 cursor-pointer transition-all duration-200 bg-gray-200/50"
            >
              <div class="flex items-center justify-between">
                <div class="flex items-center space-x-3">
                  <div class="w-10 h-10 bg-black rounded-full flex items-center justify-center">
                    <.icon name="hero-user" class="w-5 h-5 text-white" />
                  </div>

                  <div>
                    <div class="text-sm font-medium text-black">
                      {@current_user.first_name} {@current_user.last_name}
                    </div>

                    <div class="text-xs text-gray-500">{@current_user.email}</div>

                    <div class="text-xs text-black font-medium">
                      Credits: {@current_user.credits}
                    </div>
                  </div>
                </div>

                <.icon
                  name="hero-chevron-down"
                  class={"w-4 h-4 text-gray-500 transition-transform duration-200 #{if @show_profile_dropdown, do: "rotate-180", else: ""}"}
                />
              </div>
            </div>

            <%= if @show_profile_dropdown do %>
              <div class="absolute bottom-full left-0 right-0 mb-2 bg-white border border-gray-200 rounded-xl p-2 shadow-lg space-y-1">
                
<!-- Admin Section Button -->
                <%= if @current_user.email == @admin_email do %>
                  <button
                    phx-click="change_tab"
                    phx-value-tab="users"
                    class="w-full flex items-center space-x-2 px-3 py-2 text-sm text-gray-600 hover:text-black hover:bg-gray-50 rounded-lg transition-all duration-200"
                  >
                    <.icon name="hero-users" class="w-5 h-5" />
                    <span class="font-medium">Admin Section</span>
                  </button>
                <% end %>
                
<!-- Logout Link -->
                <.link href={~p"/users/log_out"} method="delete" class="w-full block">
                  <button class="w-full flex items-center space-x-2 px-3 py-2 text-sm text-gray-600 hover:text-black hover:bg-gray-50 rounded-lg transition-all duration-200">
                    <.icon name="hero-arrow-right-on-rectangle" class="w-4 h-4" />
                    <span>Logout</span>
                  </button>
                </.link>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
    
<!-- Main Content -->
    <div class="flex-1 p-8" phx-click="close_profile_dropdown">
      <!-- Get Score Tab -->
      <%= if @active_tab == "score" do %>
        <div class="space-y-8">
          <h2 class="text-3xl font-bold text-black mb-6">Website GEO Score Analysis</h2>
          
<!-- Search Form -->
          <div class="">
            <.simple_form for={@form} phx-submit="analyze_domain">
              <div class="flex space-x-4">
                <input
                  type="text"
                  value={@search_domain}
                  phx-change="update_search"
                  name="domain"
                  placeholder="Enter website domain (e.g., example.com)"
                  required
                  class="flex-1 bg-white border border-gray-300 rounded-xl px-4 py-3 text-black placeholder-gray-400 focus:outline-none focus:ring-2 focus:ring-black focus:border-transparent"
                />
                <button
                  class="px-8 py-3 bg-black text-white rounded-xl font-medium hover:bg-gray-800 transition-all duration-200 transform hover:scale-105 focus:outline-none focus:ring-2 focus:ring-gray-400 disabled:opacity-50 disabled:cursor-not-allowed"
                  disabled={@loading}
                >
                  Analyze
                </button>
              </div>
            </.simple_form>
          </div>
          
<!-- Results -->
          <%= if @show_results do %>
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-6">
              <!-- GEO Score Card -->
              <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
                <h3 class="text-xl font-semibold text-black mb-4 flex items-center">
                  <.icon name="hero-chart-bar" class="mr-2 text-black w-6 h-6" /> GEO Score
                </h3>
                <!-- Score Meter -->
                <div class="relative w-32 h-32 mx-auto">
                  <svg class="transform -rotate-90 w-32 h-32">
                    <circle
                      cx="64"
                      cy="64"
                      r="56"
                      stroke="rgba(0, 0, 0, 0.1)"
                      stroke-width="8"
                      fill="transparent"
                    />
                    <circle
                      cx="64"
                      cy="64"
                      r="56"
                      stroke={score_color(@current_score)}
                      stroke-width="8"
                      fill="transparent"
                      stroke-dasharray={2 * :math.pi() * 56}
                      stroke-dashoffset={score_circle_offset(@current_score)}
                      class="transition-all duration-1000 ease-out"
                    />
                  </svg>

                  <div class="absolute inset-0 flex items-center justify-center">
                    <div class="text-center">
                      <div class={score_color_class(@current_score)}>{@current_score}</div>

                      <div class="text-sm text-gray-600">Score</div>
                    </div>
                  </div>
                </div>

                <div class="text-center mt-4">
                  <p class="text-gray-600 text-sm">
                    Your website ranks
                    <span class={rank_color_class(@current_score)}>
                      {geo_avg(@current_score)}
                    </span>
                  </p>

                  <div class="mt-2 bg-gray-50 rounded-lg p-2">
                    <p class="text-xs text-gray-600">
                      {geo_optimization(@current_score)} GEO optimization detected
                    </p>
                  </div>
                </div>
              </div>
              
<!-- Competitors Card -->
              <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
                <h3 class="text-xl font-semibold text-black mb-4">Top Competitors</h3>

                <div class="space-y-3 max-h-64 overflow-y-auto">
                  <%= for competitor <- @competitors do %>
                    <div class="bg-gray-50 rounded-lg p-3 flex justify-between items-center">
                      <span class="text-gray-600 text-sm">{competitor}</span>
                      <%!-- <div class="flex items-center">
                        <span class="text-black font-medium mr-2"><%= competitor.score %></span>
                        <div class="w-12 h-2 bg-gray-200 rounded-full overflow-hidden">
                          <div 
                            class="h-full bg-black rounded-full transition-all duration-500"
                            style={"width: #{competitor.score}%"}
                          ></div>
                        </div>
                      </div> --%>
                    </div>
                  <% end %>
                </div>
              </div>
              
<!-- Insights Card -->
              <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
                <h3 class="text-xl font-semibold text-black mb-4">Improvement Tips</h3>

                <div class="space-y-3 max-h-64 overflow-y-auto pr-2">
                  <%= for {insight, index} <- Enum.with_index(@insights, 1) do %>
                    <div class="flex items-start space-x-3">
                      <div class="w-6 h-6 bg-black rounded-full flex items-center justify-center flex-shrink-0 mt-0.5">
                        <span class="text-white text-xs font-bold">{index}</span>
                      </div>

                      <p class="text-gray-600 text-sm leading-relaxed">{insight}</p>
                    </div>
                  <% end %>
                </div>
              </div>
            </div>
          <% else %>
            <div class="flex-1 bg-white border border-gray-200 rounded-2xl shadow-sm flex flex-col">
              <!-- Chat Messages Area -->
              <div class="flex-1 p-6 overflow-y-auto" id="chat-messages" phx-hook="ScrollToBottom">
                <div class="space-y-4">
                  <%= if @show_results == false do %>
                    <%= if @loading do %>
                      <div class="grid grid-cols-1 lg:grid-cols-3 gap-6">
                        <!-- GEO Score Card -->
                        <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
                          <h3 class="text-xl font-semibold text-black mb-4 flex items-center">
                            <div class="w-6 h-6 bg-gray-200 rounded-full animate-pulse mr-2">
                            </div>

                            <div class="h-4 w-32 bg-gray-200 rounded animate-pulse"></div>
                          </h3>
                          <!-- Score Meter -->
                          <div class="relative w-32 h-32 mx-auto">
                            <svg class="transform -rotate-90 w-32 h-32">
                              <circle
                                cx="64"
                                cy="64"
                                r="56"
                                stroke="rgba(0, 0, 0, 0.1)"
                                stroke-width="8"
                                fill="transparent"
                              />
                              <circle
                                cx="64"
                                cy="64"
                                r="56"
                                stroke="#e5e7eb"
                                stroke-width="8"
                                fill="transparent"
                                class="animate-pulse"
                              />
                            </svg>

                            <div class="absolute inset-0 flex items-center justify-center">
                              <div class="text-center">
                                <div class="h-6 w-12 bg-gray-200 rounded animate-pulse"></div>

                                <div class="h-3 w-16 bg-gray-200 rounded mt-2 animate-pulse">
                                </div>
                              </div>
                            </div>
                          </div>

                          <div class="text-center mt-4">
                            <div class="h-4 w-48 bg-gray-200 rounded mx-auto animate-pulse"></div>

                            <div class="mt-2 bg-gray-50 rounded-lg p-2">
                              <div class="h-3 w-40 bg-gray-200 rounded mx-auto animate-pulse">
                              </div>
                            </div>
                          </div>
                        </div>
                        
<!-- Competitors Card -->
                        <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
                          <h3 class="text-xl font-semibold text-black mb-4">
                            <div class="h-4 w-40 bg-gray-200 rounded animate-pulse"></div>
                          </h3>

                          <div class="space-y-3 max-h-64 overflow-y-auto">
                            <div class="bg-gray-50 rounded-lg p-3 flex justify-between items-center">
                              <div class="h-4 w-32 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="bg-gray-50 rounded-lg p-3 flex justify-between items-center">
                              <div class="h-4 w-32 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="bg-gray-50 rounded-lg p-3 flex justify-between items-center">
                              <div class="h-4 w-32 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="bg-gray-50 rounded-lg p-3 flex justify-between items-center">
                              <div class="h-4 w-32 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="bg-gray-50 rounded-lg p-3 flex justify-between items-center">
                              <div class="h-4 w-32 bg-gray-200 rounded animate-pulse"></div>
                            </div>
                          </div>
                        </div>
                        
<!-- Insights Card -->
                        <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
                          <h3 class="text-xl font-semibold text-black mb-4">
                            <div class="h-4 w-40 bg-gray-200 rounded animate-pulse"></div>
                          </h3>

                          <div class="space-y-3">
                            <div class="flex items-start space-x-3">
                              <div class="w-6 h-6 bg-gray-200 rounded-full flex-shrink-0 animate-pulse">
                              </div>

                              <div class="h-4 w-56 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="flex items-start space-x-3">
                              <div class="w-6 h-6 bg-gray-200 rounded-full flex-shrink-0 animate-pulse">
                              </div>

                              <div class="h-4 w-56 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="flex items-start space-x-3">
                              <div class="w-6 h-6 bg-gray-200 rounded-full flex-shrink-0 animate-pulse">
                              </div>

                              <div class="h-4 w-56 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="flex items-start space-x-3">
                              <div class="w-6 h-6 bg-gray-200 rounded-full flex-shrink-0 animate-pulse">
                              </div>

                              <div class="h-4 w-56 bg-gray-200 rounded animate-pulse"></div>
                            </div>

                            <div class="flex items-start space-x-3">
                              <div class="w-6 h-6 bg-gray-200 rounded-full flex-shrink-0 animate-pulse">
                              </div>

                              <div class="h-4 w-56 bg-gray-200 rounded animate-pulse"></div>
                            </div>
                          </div>
                        </div>
                      </div>
                    <% else %>
                      <!-- Welcome Message -->
                      <div class="text-center py-12">
                        <div class="w-16 h-16 bg-gray-100 rounded-full flex items-center justify-center mx-auto mb-4">
                          <.icon name="hero-chat-bubble-left-right" class="w-8 h-8 text-gray-400" />
                        </div>

                        <h3 class="text-xl font-semibold text-black mb-2">
                          Welcome to Mention Score
                        </h3>

                        <p class="text-gray-600 max-w-md mx-auto">
                          Enter your website’s domain in the field above and click Analyze. Our system will review your domain and generate a detailed report. Once the analysis is complete, you’ll be able to view your results instantly.
                        </p>
                        <!-- Quick Start Questions -->
                        <div class="mt-6 space-y-2">
                          <p class="text-sm font-medium text-gray-700 mb-3">Example Domains:</p>

                          <div class="flex flex-wrap gap-2 justify-center">
                            <button
                              phx-click="send_quick_question"
                              phx-value-question="How can I improve my website's GEO score?"
                              class="px-3 py-2 bg-gray-50 hover:bg-gray-100 text-gray-700 rounded-lg text-sm transition-all duration-200 border border-gray-200"
                            >
                              https://www.wikipedia.org/
                            </button>

                            <button
                              phx-click="send_quick_question"
                              phx-value-question="What factors affect website ranking?"
                              class="px-3 py-2 bg-gray-50 hover:bg-gray-100 text-gray-700 rounded-lg text-sm transition-all duration-200 border border-gray-200"
                            >
                              https://www.carldoe.com/
                            </button>

                            <button
                              phx-click="send_quick_question"
                              phx-value-question="How to analyze competitors?"
                              class="px-3 py-2 bg-gray-50 hover:bg-gray-100 text-gray-700 rounded-lg text-sm transition-all duration-200 border border-gray-200"
                            >
                              https://www.chatgpt.com/
                            </button>
                          </div>
                        </div>
                      </div>
                    <% end %>
                  <% end %>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      <% end %>
      
<!-- Buy Credits Tab -->
      <%= if @active_tab == "credits" && @plans != [] do %>
        <div class="space-y-8">
          <h2 class="text-3xl font-bold text-black mb-6">Choose Your Plan</h2>

          <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
            <!-- Basic Plan -->

            <%= for plan <- @plans do %>
              <div class="bg-white border border-gray-200 rounded-2xl p-6 hover:scale-105 transition-all duration-300 shadow-sm">
                <div class="text-center">
                  <h3 class="text-xl font-semibold text-black mb-2">{plan["name"]}</h3>

                  <div class="text-3xl font-bold text-black mb-1">${plan["price"] / 100}</div>

                  <p class="text-gray-500 text-sm mb-6">{plan["custom_summary"]}</p>

                  <ul class="space-y-3 mb-6 text-left">
                    <li class="flex items-center text-gray-600">
                      <.icon name="hero-check" class="w-4 h-4 text-black mr-2" /> GEO Score Checks
                    </li>

                    <li class="flex items-center text-gray-600">
                      <.icon name="hero-check" class="w-4 h-4 text-black mr-2" />
                      Basic Competitor Analysis
                    </li>

