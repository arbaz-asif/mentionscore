          </div>
        </div>
      <% end %>
      
<!-- Profile Tab -->
      <!-- Profile Tab -->
      <%= if @active_tab == "profile" do %>
        <div class="space-y-8">
          <h2 class="text-3xl font-bold text-black mb-6">Profile Settings</h2>

          <div class="grid grid-cols-1 lg:grid-cols-2 gap-6">
            <!-- User Information Card -->
            <div class="bg-white border border-gray-200 rounded-2xl p-6 shadow-sm">
              <h3 class="text-xl font-semibold text-black mb-6 flex items-center">
                <.icon name="hero-user" class="mr-2 text-black w-6 h-6" /> Account Information
